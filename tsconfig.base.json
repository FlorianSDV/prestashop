{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "importsNotUsedAsValues": "remove",
    "incremental": true,
    "preserveValueImports": false,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "node",
      "vite/client"
    ]
  },
  "exclude": [
    "../**/__tests__/*",
    "../**/*.spec.*"
  ]
}
