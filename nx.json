{
  "defaultBase": "main",
  "npmScope": "myparcel-prestashop",
  "workspaceLayout": {
    "appsDir": "views",
    "libsDir": "views"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/lib"
      ]
    },
    "build:dev": {
      "dependsOn": [
        "^build:dev"
      ],
      "outputs": [
        "{projectRoot}/lib"
      ]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build:dev"
        ],
        "parallel": 10
      }
    },
    "cloud": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build"
        ],
        "parallel": 10,
        "accessToken": "MzNmZGMyNzktM2YwOS00MzMzLThjMjctY2Q4ZjEwZWUwNGRifHJlYWQtd3JpdGU="
      }
    }
  }
}
